{
  "name": "@ar-io/arns-pilot",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "build": "yarn clean && node build.js",
    "clean": "rimraf [ dist cache ]",
    "format:check": "prettier . --check --cache",
    "format:fix": "prettier . --write",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "jest --testPathPattern=src --coverage",
    "test:integration": "jest --testPathPattern=tests/**.test.ts --runInBand",
    "test": "yarn build && jest --runInBand",
    "evolve": "yarn ts-node ./tools/evolve-contract.ts",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.15.7",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "ajv": "^8.12.0",
    "arlocal": "1.1.61",
    "arweave": "1.13.7",
    "commitlint": "^17.7.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.17.12",
    "eslint": "^6.7.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^8.0.3",
    "jest": "^27.4.3",
    "lint-staged": "^14.0.1",
    "prettier": "^2.2.1",
    "replace-in-file": "^6.2.0",
    "rimraf": "^5.0.5",
    "smartweave": "^0.4.49",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "typescript": "4.3.5",
    "warp-contracts": "^1.4.18",
    "warp-contracts-plugin-deploy": "^1.0.1"
  },
  "resolutions": {
    "arweave": "1.13.7",
    "arlocal@1.1.61": "patch:arlocal@npm%3A1.1.61#./.yarn/patches/arlocal-npm-1.1.61-aecd7b9946.patch"
  },
  "lint-staged": {
    "**/*.{ts,js,json}": [
      "eslint --fix .",
      "prettier --write ."
    ]
  },
  "packageManager": "yarn@3.6.4"
}
